    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch ",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}/dapp"
        },
        {
            "type": "chrome",
            "request": "attach",
            "name": "Attach to browser",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}/dapp",
            //退出chrome，然后
            //以如下方式启动  /Applications/Google\ Chrome.app/Contents/MacOS/Google\ Chrome --remote-debugging-port=9222 
            "port": 9222
        }
    ]
}